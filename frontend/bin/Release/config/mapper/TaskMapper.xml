<?xml version="1.0" encoding="utf-8" ?>
<sqlMap namespace="frontend.cc.gtscloud.oilpainter.dao.model.mapper" xmlns="http://ibatis.apache.org/mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <alias>
    <typeAlias alias="TaskModel" type="frontend.cc.gtscloud.oilpainter.dao.model.TaskModel" />
  </alias>
  <resultMaps>
  </resultMaps>
  <statements>
    <select id="SelectTasksWithParam" resultClass="TaskModel">
      SELECT
        [LineName],
        [ShiftName],
        [ProductName],
        [Supplier],
        [CustomerName],
        [MainPartVendor],
        [MainPartNumber],
        [MainLotNumber],
        [ActualMainPartWeight],
        [MainHolderWeight],
        [GuHuaPartVendor],
        [GuHuaPartNumber],
        [GuHuaLotNumber],
        [GuHuaActualWeight],
        [GuHuaHolderWeight],
        [GuHuaRate],
        [GuHuaPercent],
        [XiShiVendor],
        [XiShiPartNumber],
        [XiShiLotNumber],
        [XiShiWeightSPECU],
        [XiShiWeightSPECL],
        [XiShiActualWeight],
        [XiShiHolderWeight],
        [XiShiRate],
        [ActualDelaySeconds],
        [ActualSpeed] 
      FROM
        [SprayLacquerManagement].[dbo].[task], [SprayLacquerManagement].[dbo].[BOMINfo]
      WHERE
        [SprayLacquerManagement].[dbo].[task].[BOMID]=[SprayLacquerManagement].[dbo].[BOMINFO].[BOMID] AND
        1 = 1 AND
        [ShiftName] LIKE '%$ShiftName$%' AND
        [LineName] LIKE '%$LineName$%' AND
        [ProductName] LIKE '%$ProductName$%' AND
        [MainPartNumber] LIKE '%$MainPartNumber$%' AND
        [Operator] LIKE '%$Operator$%' AND
        [Supplier] LIKE '%$Supplier$%' AND
        [Status] LIKE '%$Status$%' AND
        <![CDATA[ 
    [SysDateTime] >= #StartDate# AND
        [SysDateTime] <= #DeadDate# ]]>
    </select>
    <insert id="InsertNewTask" parameterClass="TaskModel">
      INSERT INTO [SprayLacquerManagement].[dbo].[Task](
        [TaskID],
        [BomId],
        [ShiftName],
        [LineName],
        [MainPartVendor],
        [MainPartNumber],
        [MainLotNumber],
        [TargetMainPartWeight],
        [ActualMainPartWeight],
        [GuHuaPartVendor],
        [GuHuaPartNumber],
        [GuHuaLotNumber],
        [GuHuaWeightSPECL],
        [GuHuaWeightSPECU],
        [GuHuaActualWeight],
        [GuHuaRate],
        [XiShiVendor],
        [XiShiPartNumber],
        [XiShiLotNumber],
        [XiShiWeightSPECL],
        [XiShiWeightSPECU],
        [XiShiActualWeight],
        [XiShiRate],
        [ActualSpeed],
        [Status],
        [GuHuaRangeBase],
        [MainHolderWeight],
        [GuHuaHolderWeight],
        [XiShiHolderWeight],
        [ActualDelaySeconds],
        [Operator],
        [SysDateTime],
        [PartNumber]
      )
      VALUES(
        #TaskID#,
        #BomId#,
        #ShiftName#,
        #LineName#,
        #MainPartVendor#,
        #MainPartNumber#,
        #MainLotNumber#,
        #TargetMainPartWeight#,
        #ActualMainPartWeight#,
        #GuHuaPartVendor#,
        #GuHuaPartNumber#,
        #GuHuaLotNumber#,
        #GuHuaWeightSPECL#,
        #GuHuaWeightSPECU#,
        #GuHuaActualWeight#,
        #GuHuaRate#,
        #XiShiVendor#,
        #XiShiPartNumber#,
        #XiShiLotNumber#,
        #XiShiWeightSPECL#,
        #XiShiWeightSPECU#,
        #XiShiActualWeight#,
        #XiShiRate#,
        #ActualSpeed#,
        #Status#,
        #GuHuaRangeBase#,
        #MainHolderWeight#,
        #GuHuaHolderWeight#,
        #XiShiHolderWeight#,
        #ActualDelaySeconds#,
        #Operator#,
        #SysDateTime#,
        #PartNumber#
      )
    </insert>
    <update id="UpdateTask" parameterClass="TaskModel">
      UPDATE [SprayLacquerManagement].[dbo].[Task]
	      SET 
          [GuHuaPartVendor] = #GuHuaPartVendor#,
	        [GuHuaPartNumber] = #GuHuaPartNumber#,
	        [GuHuaLotNumber] = #GuHuaLotNumber#,
	        [GuHuaWeightSPECL] = #GuHuaWeightSPECL#,
	        [GuHuaWeightSPECU] = #GuHuaWeightSPECU#,
	        [GuHuaActualWeight] = #GuHuaActualWeight#,
	        [GuHuaRate] = #GuHuaRate#,
	        [XiShiVendor] = #XiShiVendor#,
	        [XiShiPartNumber] = #XiShiPartNumber#,
	        [XiShiWeightSPECL] = #XiShiWeightSPECL#,
	        [XiShiWeightSPECU] = #XiShiWeightSPECU#,
	        [XiShiActualWeight] = #XiShiActualWeight#,
	        [XiShiRate] = #XiShiRate#,
	        [ActualSpeed] = #ActualSpeed#,
	        [Status] = #Status#,
	        [GuHuaHolderWeight] = #GuHuaHolderWeight#,
	        [XiShiHolderWeight] = #XiShiHolderWeight#,
	        [GuHuaRangeBase] = #GuHuaRangeBase#,
	        [ActualDelaySeconds] = #ActualDelaySeconds#,
	        [Operator] = #Operator#,
	        [SysDateTime] = #SysDateTime#
        WHERE 
	        [TaskID] = #TaskID# AND
	        [BomId] = #BomId#
    </update>
  </statements>
</sqlMap>